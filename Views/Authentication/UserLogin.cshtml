@model UserLoginViewModel
<div>Logga in</div>

<form method="post" asp-controller="Authentication" asp-action="UserLogin" novalidate>
    <span asp-validation-for="Email"></span>
    <input type="email" asp-for="Email" placeholder="Email" />

    <span asp-validation-for="Password"></span>
    <input type="password" asp-for="Password" placeholder="Lösenord" />

    <button type="submit">Logga in</button>

    <div asp-validation-summary="ModelOnly"></div>
</form>